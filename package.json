{
  "name": "soundcore-manager",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build-bindings": "cargo test --manifest-path ./src-tauri/Cargo.toml",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "generate-types": "yarn typeshare:tauri && yarn typeshare:soundcore-lib",
    "typeshare:soundcore-lib": "typeshare ./soundcore-lib --lang=typescript --output-file=./src/types/soundcore-lib.d.ts",
    "typeshare:tauri": "typeshare ./src-tauri --lang=typescript --output-file=./src/types/tauri-backend.d.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.0",
    "@tanstack/react-query": "^4.20.4",
    "@tanstack/react-query-devtools": "^4.20.4",
    "@tauri-apps/api": "^1.2.0",
    "chart.js": "^4.3.0",
    "chartjs-plugin-dragdata": "^2.2.4",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.0.0",
    "react-dom": "^18.2.0",
    "tauri-plugin-log-api": "https://github.com/tauri-apps/tauri-plugin-log",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.2.0",
    "@types/node": "^18.7.10",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^4.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.1"
  }
}
